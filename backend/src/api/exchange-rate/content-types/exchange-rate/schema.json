{
  "kind": "collectionType",
  "collectionName": "exchange_rates",
  "info": {
    "singularName": "exchange-rate",
    "pluralName": "exchange-rates",
    "displayName": "Exchange Rate"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "from_currency": {
      "type": "string",
      "required": true
    },
    "to_currency": {
      "type": "string",
      "required": true
    },
    "rate": {
      "type": "decimal",
      "required": true
    },
    "buy_price": {
      "type": "decimal",
      "required": true
    },
    "sell_price": {
      "type": "decimal",
      "required": true
    },
    "open_rate": {
      "type": "decimal"
    },
    "high_rate": {
      "type": "decimal"
    },
    "low_rate": {
      "type": "decimal"
    },
    "volume": {
      "type": "decimal"
    },
    "change_percentage": {
      "type": "decimal"
    },
    "source": {
      "type": "string",
      "required": true
    },
    "timestamp": {
      "type": "datetime",
      "required": true
    },
    "market_multiplier": {
      "type": "decimal",
      "default": 1
    },
    "is_crypto": {
      "type": "boolean",
      "default": false
    }
  }
}