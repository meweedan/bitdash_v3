{
  "kind": "collectionType",
  "collectionName": "auto_dealers",
  "info": {
    "singularName": "auto-dealer",
    "pluralName": "auto-dealers",
    "displayName": "Auto Dealer"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "dealershipName": {
      "type": "string",
      "required": true
    },
    "dealershipAddress": {
      "type": "string",
      "required": true
    },
    "dealerRating": {
      "type": "decimal"
    },
    "commission_rate": {
      "type": "decimal",
      "default": 5.0
    },
    "status": {
      "type": "enumeration",
      "enum": ["active", "pending", "suspended"],
      "default": "pending"
    },
    "operator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::operator.operator"
    },
    "vehicles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vehicle.vehicle",
      "mappedBy": "dealer"
    },
    "inquiries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vehicle-inquiry.vehicle-inquiry",
      "mappedBy": "dealer"
    },
    "salesHistory": {
      "type": "json"
    },
    "certifications": {
      "type": "json"
    },
    "specializations": {
      "type": "enumeration",
      "enum": ["luxury", "sports", "commercial", "general"],
      "default": "general"
    },
    "availableFinancing": {
      "type": "boolean",
      "default": false
    },
    "insuranceProvider": {
      "type": "boolean",
      "default": false
    }
  }
}