{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "menu": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu.menu"
    },
    "restaurant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::restaurant.restaurant"
    },
    "tables": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::table.table",
      "mappedBy": "user"
    },
    "customer_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer-profile.customer-profile",
      "mappedBy": "users_permissions_user"
    },
    "operator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::operator.operator",
      "mappedBy": "users_permissions_user"
    },
    "agent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::agent.agent",
      "mappedBy": "users_permissions_user"
    },
    "merchant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::merchant.merchant",
      "mappedBy": "users_permissions_user"
    },
    "captain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::captain.captain",
      "mappedBy": "users_permissions_user"
      },
    "employer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::employer.employer",
      "mappedBy": "users_permissions_user"
    },
    "shop_owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop-owner.shop-owner",
      "mappedBy": "users_permissions_user"
    }
  }
}
